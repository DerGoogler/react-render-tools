{
  "name": "react-render-tools",
  "version": "1.0.1",
  "description": "Simple tools to make react render easier to use",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "module": "dist/onuix.esm.js",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "web:dev": "webpack --config webpack.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "resolutions": {
    "**/typescript": "^4.1.3"
  },
  "keywords": [
    "react",
    "render",
    "tools",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DerGoogler/react-render-tools"
  },
  "author": "Der_Googler",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/DerGoogler/react-render-tools/issues"
  },
  "homepage": "https://github.com/DerGoogler/react-render-tools",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.6",
    "postcss": "^8.4.16",
    "rollup-plugin-postcss": "^4.0.2",
    "ts-loader": "^9.3.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "files": [
    "/dist"
  ]
